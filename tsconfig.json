{
    "compilerOptions": {
        "target": "esnext",
        "module": "CommonJS",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "lib",
        "declaration": true
    },
    "include": ["lib-src/embedded.ts"],
    "exclude": ["node_modules"]
}
